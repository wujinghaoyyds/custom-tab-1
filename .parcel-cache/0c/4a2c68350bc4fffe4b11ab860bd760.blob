* {
    box-sizing: border-box;
    margin: 0;
    padding: 0;
}
body {
    width: 100vw;
    min-width:500px;
    height: 100vh;
    position: relative;
    background-image: url('bf82c59cfaf801bbadfdd3d1e382c550');
    /*background-repeat: no-repeat;*/

    /*background-size: cover;*/
    /*background-position: center 0;*/
}
/*vertical-align:middle*/
#formSearch {
    position: absolute;
    background: #dcc812;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    display:flex;
}
#searchEngine {
    height: 50px;
    width:50px;
    border:0;
    outline: none;
    border-radius: 25px;
}
#inputSearch {
    background: #765959;
    border:0;
    width: 70vw;
    max-width:350px;
    min-width:200px;
    height: 50px;
    outline: none;
    padding-left:10px;
    font-size:16px;
}
#clearInput {
    background: #ac301a;
    border:0;
    height: 50px;
    width:50px;
    outline: none;
}
#buttonSearch {
    border:0;
    height: 50px;
    width:50px;
    outline: none;
    background: #5068a3;
    border-radius: 0 25px 25px 0;
}
img{
    width: 100px;
    height:100px;
}